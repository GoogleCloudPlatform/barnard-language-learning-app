  <file source-language="en" datatype="plaintext" original="ng2.template">
    <body>
      <trans-unit id="dir" datatype="html">
        <target>ltr</target>
      </trans-unit>
      <trans-unit id="introAbout" datatype="html">
        <target>Google Arts &amp; Culture ने Woolaroo नाम का एक प्रयोग शुरू किया है जो मशीन लर्निंग का इस्तेमाल करता है,
          ताकि नए और इंटरैक्टिव तरीके से स्थानीय भाषा इस्तेमाल की जा सके.
        </target>
      </trans-unit>
      <trans-unit id="next" datatype="html">
        <target>आगे बढ़ें</target>
      </trans-unit>
      <trans-unit id="skip" datatype="html">
        <target>अभी नहीं</target>
      </trans-unit>
      <trans-unit id="introFeedback" datatype="html">
        <target>Woolaroo, रीयल टाइम में बेहतर मशीन लर्निंग का इस्तेमाल करके, आपकी फ़ोटो में मौजूद चीज़ों को समझता है और
          उनका अनुवाद करता है. ज़रूरी नहीं की यह हर बार सही ही हो. आप अपना सुझाव देकर इसे बेहतर बनाने में मदद कर सकते
          हैं.
        </target>
      </trans-unit>
      <trans-unit id="startExploring" datatype="html">
        <target>एक्सप्लोर करना शुरू करें</target>
      </trans-unit>
      <trans-unit id="termsAndPrivacyTitle" datatype="html">
        <target>शर्तें और निजता</target>
      </trans-unit>
      <trans-unit id="introTermsAndPrivacy" datatype="html">
        <target>प्रोजेक्ट Woolaroo का इस्तेमाल करके आप<x id="START_LINK" ctype="x-a" equiv-text="&lt;a&gt;"/>नियमों
          <x id="CLOSE_LINK" ctype="x-a" equiv-text="&lt;/a&gt;"/>
          और शर्तों के साथ ही,<x id="START_LINK" ctype="x-a" equiv-text="&lt;a&gt;"/>निजता नीति से
          <x id="CLOSE_LINK" ctype="x-a" equiv-text="&lt;/a&gt;"/>
          सहमत होते हैं.
        </target>
      </trans-unit>
      <trans-unit id="accept" datatype="html">
        <target>स्वीकार करें</target>
      </trans-unit>
      <trans-unit id="photoSource" datatype="html">
        <target>आप अपने फ़ोन में मौजूद फ़ोटो इस्तेमाल कर सकते हैं या अपने आस-पास की किसी चीज़ की फ़ोटो लें</target>
      </trans-unit>
      <trans-unit id="pickPhoto" datatype="html">
        <target>फ़ोटो चुनें</target>
      </trans-unit>
      <trans-unit id="takePhoto" datatype="html">
        <target>फ़ोटो लें</target>
      </trans-unit>
      <trans-unit id="errorTitle" datatype="html">
        <target>गड़बड़ी</target>
      </trans-unit>
      <trans-unit id="close" datatype="html">
        <target>बंद करें</target>
      </trans-unit>
      <trans-unit id="imageCaptionLabel" datatype="html">
        <target>यह क्या है?</target>
      </trans-unit>
      <trans-unit id="imageCaptionRequired" datatype="html">
        <target>इमेज का कैप्शन देना ज़रूरी है</target>
      </trans-unit>
      <trans-unit id="translationFeedback" datatype="html">
        <target>अनुवाद के लिए सुझाव</target>
      </trans-unit>
      <trans-unit id="feedbackTitle" datatype="html">
        <target>सुझाव</target>
      </trans-unit>
      <trans-unit id="submit" datatype="html">
        <target>सबमिट करें</target>
      </trans-unit>
      <trans-unit id="feedbackInstructions" datatype="html">
        <target>हमारे साथ सुझाव शेयर करके, प्रोजेक्ट Woolaroo से किए गए अनुवाद को बेहतर बनाने में हमारी मदद करें:</target>
      </trans-unit>
      <trans-unit id="incorrectTranslation" datatype="html">
        <target>गलत अनुवाद</target>
      </trans-unit>
      <trans-unit id="offensiveTranslation" datatype="html">
        <target>आपत्तिजनक अनुवाद</target>
      </trans-unit>
      <trans-unit id="alternateTranslation" datatype="html">
        <target>अलग तरह का अनुवाद</target>
      </trans-unit>
      <trans-unit id="other" datatype="html">
        <target>अन्य</target>
      </trans-unit>
      <trans-unit id="feedbackTypeRequired" datatype="html">
        <target>यह नहीं चुना गया कि सुझाव किस तरह का है</target>
      </trans-unit>
      <trans-unit id="enterFeedback" datatype="html">
        <target>सुझाव दें</target>
      </trans-unit>
      <trans-unit id="feedbackRequired" datatype="html">
        <target>सुझाव देना ज़रूरी है</target>
      </trans-unit>
      <trans-unit id="addWord" datatype="html">
        <target>शब्द जोड़ें</target>
      </trans-unit>
      <trans-unit id="add" datatype="html">
        <target>जोड़ें</target>
      </trans-unit>
      <trans-unit id="addWordInstructions" datatype="html">
        <target>
          <x id="INTERPOLATION" equiv-text="{{endangeredLanguage}}"/>
          के नए शब्द और उनके अनुवाद जोड़कर ऐप्लिकेशन में योगदान दें.
        </target>
      </trans-unit>
      <trans-unit id="wordInNativeScript" datatype="html">
        <target>शब्द, स्थानीय लिपि में</target>
      </trans-unit>
      <trans-unit id="nativeWordRequired" datatype="html">
        <target>शब्द, मूल स्क्रिप्ट में होना चाहिए</target>
      </trans-unit>
      <trans-unit id="nativeScriptHint" datatype="html">
        <target>उदाहरण.. هتاف للترحيب</target>
      </trans-unit>
      <trans-unit id="wordInEnglish" datatype="html">
        <target>शब्द, अंग्रेज़ी में</target>
      </trans-unit>
      <trans-unit id="englishWordRequired" datatype="html">
        <target>शब्द, अंग्रेज़ी में होना चाहिए</target>
      </trans-unit>
      <trans-unit id="englishWordHint" datatype="html">
        <target>उदाहरण, &apos;hello&apos;</target>
      </trans-unit>
      <trans-unit id="transliteration" datatype="html">
        <target>ट्रांसलिट्रेशन (ज़रूरी नहीं)</target>
      </trans-unit>
      <trans-unit id="transliterationHint" datatype="html">
        <target>लैटिन अक्षर में शब्द, उदाहरण के लिए &apos;marhaba&apos;</target>
      </trans-unit>
      <trans-unit id="recordPronunciation" datatype="html">
        <target>उच्चारण रिकॉर्ड करें</target>
      </trans-unit>
      <trans-unit id="saved" datatype="html">
        <target>सेव किया गया</target>
      </trans-unit>
      <trans-unit id="specialCharsInstructions" datatype="html">
        <target>विशेष वर्ण टाइप करना चाहते हैं?<x id="START_LINK" ctype="x-a" equiv-text="&lt;a&gt;"/>GBoard
          <x id="CLOSE_LINK" ctype="x-a" equiv-text="&lt;/a&gt;"/>
          या<x id="START_LINK_1" ctype="x-a" equiv-text="&lt;a&gt;"/>Keyman
          <x id="CLOSE_LINK" ctype="x-a" equiv-text="&lt;/a&gt;"/>
          इस्तेमाल करें
        </target>
      </trans-unit>
      <trans-unit id="aboutTitle" datatype="html">
        <target>के बारे में जानकारी</target>
      </trans-unit>
      <trans-unit id="aboutContent1" datatype="html">
        <target>Woolaroo, Google Arts &amp; Culture का ओपन सोर्स वाला प्रयोग है, जो आपके फ़ोन के कैमरे का इस्तेमाल करके
          फ़ोटो में मौजूद चीज़ों का स्थानीय भाषा में अनुवाद करता है.
        </target>
      </trans-unit>
      <trans-unit id="aboutContent2" datatype="html">
        <target>यह बहुत आसान है - ऑब्जेक्ट की फ़ोटो लें या खुद कोई फ़ोटो अपलोड करें और वास्तविक समय में अनुवाद पाएं. आप
          अनुवाद वाली इमेज सेव कर सकते हैं और शेयर कर सकते है या फिर से कोशिश कर सकते हैं.
        </target>
      </trans-unit>
      <trans-unit id="technologyTitle" datatype="html">
        <target>टेक्नोलॉजी</target>
      </trans-unit>
      <trans-unit id="technologyContent1" datatype="html">
        <target>Woolaroo, वास्तविक समय में आपकी फ़ोटो में मौजूद चीज़ों को समझने और उनका अनुवाद करने के लिए, बेहतर मशीन लर्निंग का इस्तेमाल करता है.</target>
      </trans-unit>
      <trans-unit id="technologyContent2" datatype="html">
        <target>अनुवाद, इमेज में मौजूद खास ऑब्जेक्ट के हिसाब से होगा. अगर कई ऑब्जेक्ट मिलते हैं, तो आप स्क्रोल करके सही अनुवाद चुन सकते हैं.</target>
      </trans-unit>
      <trans-unit id="technologyContent3" datatype="html">
        <target>प्रोजेक्ट Woolaroo, एकदम नई एआई (AI) टेक्नोलॉजी से काम करता है, इसलिए हो सकता है कि हर बार यह सही न हो.
          लेकिन, इसे बेहतर बनाने के लिए हम लगातार कोशिश कर रहे हैं. कोई अनुवाद सही न लगने पर, अनुवाद के लिए सुझाव सुविधा
          का इस्तेमाल करके आप हमारी मदद कर सकते हैं
        </target>
      </trans-unit>
      <trans-unit id="logo" datatype="html">
        <target>Woolaroo</target>
      </trans-unit>
      <trans-unit id="addToHomeScreen" datatype="html">
        <target>होम स्क्रीन पर जोड़ें</target>
      </trans-unit>
      <trans-unit id="enterSomethingElse" datatype="html">
        <target>कुछ और लिखें</target>
      </trans-unit>
      <trans-unit id="wordSubmitted" datatype="html">
        <target>समीक्षा के लिए सबमिट किया गया</target>
      </trans-unit>
      <trans-unit id="addWordError" datatype="html">
        <target>शब्द जोड़ा नहीं जा सकता</target>
      </trans-unit>
      <trans-unit id="startCameraError" datatype="html">
        <target>कैमरा चालू नहीं किया जा सका</target>
      </trans-unit>
      <trans-unit id="captureImageError" datatype="html">
        <target>इमेज कैप्चर नहीं की जा सकी</target>
      </trans-unit>
      <trans-unit id="feedbackSubmitted" datatype="html">
        <target>सुझाव सबमिट किया गया</target>
      </trans-unit>
      <trans-unit id="submitFeedbackError" datatype="html">
        <target>सुझाव सेव नहीं किया जा सका</target>
      </trans-unit>
    </body>
